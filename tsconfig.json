{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "./",
    "outDir": "./dist",
    "typeRoots": ["./src/@types", "tests", "./node_modules/@types"],
    "types": ["node", "express"],

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": false
  },
  "include": ["src/**/*", "prisma/**/*", "src/@types/**/*"]
}
