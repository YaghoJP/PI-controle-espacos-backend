{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "./dist",
    "typeRoots": ["./src/@types", "./node_modules/@types"],
    "types": ["node", "express"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": false
  },
  "include": ["src/**/*", "prisma/**/*"]
}
